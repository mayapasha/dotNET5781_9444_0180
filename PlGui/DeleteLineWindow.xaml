<Window x:Class="PlGui.DeleteLineWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PlGui"
        mc:Ignorable="d"
        Title="DeleteLineWindow" Height="160" Width="310">
    <Grid>
        <Label  Content="Enter line to delete" HorizontalAlignment="Left" Margin="10,21,0,0" VerticalAlignment="Top" Height="31" Width="119"/>
        <Button x:Name="b_delete_line" Content="delete" HorizontalAlignment="Left" Margin="208,87,0,0" VerticalAlignment="Top" Width="75" Click="b_delete_line_Click"/>
        <ComboBox x:Name="cb_lineDelete" HorizontalAlignment="Left" Margin="134,21,0,0" VerticalAlignment="Top" Width="149" Height="31"/>

    </Grid>
</Window>
